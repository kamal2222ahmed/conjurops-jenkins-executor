{
  "conjur": {
    "host_factory_token": "%%HOST_TOKEN%%",
    "host_identity": {
      "id": "ec2/%%HOST_ID%%"
    },
    "configuration": {
      "appliance_url": "<%= @appliance_url %>",
      "account": "<%= @account %>",
      "ssl_certificate": "<%= @ssl_certificate.split("\n").join("\\n") %>"
    }
  },
  "run_list": "conjur-host-identity"
}
